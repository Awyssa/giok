{
	"name": "bun-react-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "src/index.tsx",
	"module": "src/index.tsx",
	"scripts": {
		"router:generate": "bun scripts/generate-routes.ts",
		"dev": "bun run router:generate && bun --hot --no-clear-screen src/index.tsx",
