		"@tanstack/react-router-devtools": "^1.146.2",
		"@types/jsonwebtoken": "^9.0.10",
		"bun-plugin-tailwind": "^0.0.14",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.7",
		"elysia": "^1.3.5",
		"eslint": "^9.39.1",
		"jsonwebtoken": "^9.0.3",
		"pg": "^8.16.3",
		"react": "^19",
		"react-dom": "^19",
		"react-feather": "^2.0.10",
